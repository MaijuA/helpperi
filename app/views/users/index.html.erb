<h1>Käyttäjien listaus</h1>

<table>
  <thead>
    <tr>
      <th>Käyttäjätunnus</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= link_to 'Näytä koko profiili', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>
